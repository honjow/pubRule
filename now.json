{
    "name": "surgio-api-pub",
    "version": 2,
    "public": false,
    "env": {
      "SURGIO_REMOTE_SNIPPET_CACHE_MAXAGE": "60000",
      "SURGIO_PROVIDER_CACHE_MAXAGE": "60000"
    },
    "build": {
      "env": {
        "SURGIO_REMOTE_SNIPPET_CACHE_MAXAGE": "60000",
        "SURGIO_PROVIDER_CACHE_MAXAGE": "60000"
      }
    },
    "builds": [
      { 
        "src": "gateway.js",
        "use": "@now/node",
        "config": {
          "includeFiles": [
            "provider/**",
            "template/**",
            "*.js",
            "*.json"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "methods": ["HEAD", "GET"],
        "dest": "/gateway.js"
      }
    ]
  }